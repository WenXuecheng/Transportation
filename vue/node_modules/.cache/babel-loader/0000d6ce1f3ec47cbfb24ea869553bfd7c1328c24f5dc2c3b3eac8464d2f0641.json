d>\r\n                    <el-form-item prop=\"date1\">\r\n                        <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"ruleForm.date1\" style=\"width: 25%;\"></el-date-picker>\r\n                    </el-form-item>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"寄件数量\" prop=\"delivery\">\r\n                    <template>\r\n                        <template>\r\n                            <el-input-number v-model=\"num\" :step=\"2\" step-strictly></el-input-number>\r\n                        </template>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"其他\" prop=\"resource\">\r\n                    <el-radio-group v-model=\"ruleForm.resource\">\r\n                        <el-radio label=\"自提\"></el-radio>\r\n                        <el-radio label=\"保险\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"姓名\" prop=\"delivery\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"text\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人电话\" prop=\"delivery\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"text\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人邮箱\" prop=\"delivery\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n    