{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { defineComponent } from 'vue';\nexport default defineComponent({\n  name: \"order\",\n  data() {\n    return {\n      tableData: [],\n      centerDialogVisible: false,\n      uploadUsername: '',\n      uploadOrdername: '',\n      user: '',\n      fileList: [],\n      currentOrdername: '',\n      limitnum: 10,\n      UserDialogVisible: false,\n      urls: [],\n      AdminDialogVisible: false,\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      username: '',\n      name: '',\n      options: [{\n        value: 1,\n        label: '订单已提交',\n        disabled: true\n      }, {\n        value: 2,\n        label: '订单等待付款',\n        disabled: true\n      }, {\n        value: 3,\n        label: '已发出'\n      }, {\n        value: 4,\n        label: '已到达'\n      }, {\n        value: 5,\n        label: '待取件'\n      }, {\n        value: 6,\n        label: '已取件'\n      }],\n      options1: [{\n        value: 0,\n        label: '否'\n      }, {\n        value: 1,\n        label: '是'\n      }],\n      editDialogVisible: false,\n      form: {}\n    };\n  },\n  created() {\n    this.user = localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {};\n    this.searchOrdersByUsername();\n  },\n  methods: {\n    //上传照片\n    handleAddPhoto(ordername) {\n      this.centerDialogVisible = true;\n      this.currentOrdername = ordername;\n    },\n    updateData() {\n      this.searchOrdersByUsername();\n    },\n    deleteOrder(orderName) {\n      try {\n        this.request.delete(\"/order/ordername/\" + orderName).then(res => {\n          if (res.code === '200') {\n            this.$message.success(\"删除成功\");\n            this.updateData();\n          }\n        });\n      } catch (e) {\n        this.$message.error(\"删除失败\");\n      }\n    },\n    uploadFileError(err, file, fileList) {\n      this.$message.error(\"上传失败！\");\n    },\n    exceedFile(files, fileList) {\n      this.$message.error('只能上传' + this.limitnum + '个文件');\n    },\n    removeFile(file, fileList) {\n      this.fileList = fileList;\n    },\n    loadJsonFromFile(file, fileList) {\n      this.fileList = fileList;\n    },\n    onBeforeUploadImage(file) {\n      const isIMAGE = file.type === 'image/jpeg' || 'image/jpg' || 'image/png';\n      const isLt1M = file.size / 1024 / 1024 < 5;\n      if (!isIMAGE) {\n        this.$message.error('上传文件只能是图片格式!');\n      }\n      if (!isLt1M) {\n        this.$message.error('上传文件大小不能超过 1MB!');\n      }\n      return isIMAGE && isLt1M;\n    },\n    uploadSuccess(response, file, fileList) {\n      let order_file = {\n        \"orderName\": this.currentOrdername,\n        \"userName\": '',\n        \"url\": response\n      };\n      try {\n        this.request.post(\"/order-file\", order_file).then(res => {\n          if (res.code === '200') {\n            this.searchOrdersByUsername();\n            this.$message.success(\"上传成功\");\n          }\n        });\n        this.searchOrdersByUsername();\n      } catch (e) {\n        this.$message.error(\"上传失败\");\n        this.request.delete(\"/file/url/\" + response).then(res => {});\n      }\n    },\n    //加载表单\n    searchOrdersByUsername() {\n      this.request.get(\"/order/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          name: this.name\n        }\n      }).then(res => {\n        res = JSON.parse(JSON.stringify(res.data));\n        this.tableData = res.records;\n        this.pageNum = res.pages;\n        this.total = res.total;\n        console.log(res);\n        console.log(this.tableData);\n        const newProperty1 = 'userUpload';\n        const newProperty2 = 'adminUpload';\n        for (let i = 0; i < this.tableData.length; i++) {\n          this.request.get(\"/order-file/ordername/\" + this.tableData[i].orderName).then(res => {\n            this.tableData[i][newProperty1] = [];\n            this.tableData[i][newProperty2] = [];\n            if (res.data != null) {\n              for (let k = 0; k < res.data.length; k++) {\n                if (res.data[k].adminUp === false) {\n                  this.tableData[i][newProperty1].push(res.data[k].url);\n                } else {\n                  this.tableData[i][newProperty2].push(res.data[k].url);\n                }\n              }\n            }\n          });\n          ///保险自提\n          this.tableData[i].others = [this.tableData[i].others[0] + this.tableData[i].others[1], this.tableData[i].others[3] + this.tableData[i].others[4]];\n          //是否确认\n          if (this.tableData[i].isConfirmed === true) {\n            this.tableData[i].isConfirmed = '是';\n          } else {\n            this.tableData[i].isConfirmed = '否';\n          }\n        }\n      });\n    },\n    showUserPhoto(urls) {\n      this.UserDialogVisible = true;\n      this.urls = urls;\n    },\n    showAdminPhoto(urls) {\n      this.AdminDialogVisible = true;\n      this.urls = urls;\n    },\n    confirmeOrder(order) {\n      order = {\n        id: order.id,\n        orderName: order.orderName,\n        isConfirmed: true\n      };\n      console.log(order);\n      this.request.post(\"/order/update\", order).then(res => {\n        if (res.code === '200') {\n          this.searchOrdersByUsername();\n          this.$message.success(\"保存成功\");\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.searchOrdersByUsername();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.searchOrdersByUsername();\n    },\n    reset() {\n      this.username = \"\";\n      this.name = \"\";\n      this.searchOrdersByUsername();\n    },\n    //编辑订单\n    editOrder(order) {\n      this.editDialogVisible = true;\n      this.form.id = order.id;\n      this.form.orderName = order.orderName;\n      this.form.isConfirmed = order.isConfirmed;\n      this.form.oderStatus = order.oderStatus;\n      this.form.email = order.email;\n      this.form.phone = order.phone;\n      this.form.receiveAdress = order.receiveAdress;\n    },\n    updateOrder() {\n      console.log(this.form);\n      // this.request.post(\"/order/update\", this.form).then(res => {\n      //     if (res.code === '200') {\n      //         this.searchOrdersByUsername()\n      //         this.editDialogVisible = false\n      //         this.$message.success(\"上传成功\")\n      //\n      //     }\n      // })\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","name","data","tableData","centerDialogVisible","uploadUsername","uploadOrdername","user","fileList","currentOrdername","limitnum","UserDialogVisible","urls","AdminDialogVisible","pageNum","pageSize","total","username","options","value","label","disabled","options1","editDialogVisible","form","created","localStorage","getItem","JSON","parse","searchOrdersByUsername","methods","handleAddPhoto","ordername","updateData","deleteOrder","orderName","request","delete","then","res","code","$message","success","e","error","uploadFileError","err","file","exceedFile","files","removeFile","loadJsonFromFile","onBeforeUploadImage","isIMAGE","type","isLt1M","size","uploadSuccess","response","order_file","post","get","params","stringify","records","pages","console","log","newProperty1","newProperty2","i","length","k","adminUp","push","url","others","isConfirmed","showUserPhoto","showAdminPhoto","confirmeOrder","order","id","msg","handleSizeChange","handleCurrentChange","reset","editOrder","oderStatus","email","phone","receiveAdress","updateOrder"],"sources":["src/views/Order.vue"],"sourcesContent":["<script>\r\nimport {defineComponent} from 'vue'\r\n\r\nexport default defineComponent({\r\n    name: \"order\",\r\n    data() {\r\n        return {\r\n            tableData: [],\r\n            centerDialogVisible: false,\r\n            uploadUsername: '',\r\n            uploadOrdername: '',\r\n            user: '',\r\n            fileList: [],\r\n            currentOrdername: '',\r\n            limitnum: 10,\r\n            UserDialogVisible: false,\r\n            urls: [],\r\n            AdminDialogVisible: false,\r\n            pageNum: 1,\r\n            pageSize: 10,\r\n            total: 0,\r\n            username: '',\r\n            name: '',\r\n            options: [{\r\n                value: 1,\r\n                label: '订单已提交',\r\n                disabled: true\r\n            }, {\r\n                value: 2,\r\n                label: '订单等待付款',\r\n                disabled: true\r\n            }, {\r\n                value: 3,\r\n                label: '已发出'\r\n            }, {\r\n                value: 4,\r\n                label: '已到达'\r\n            }, {\r\n                value: 5,\r\n                label: '待取件'\r\n            }, {\r\n                value: 6,\r\n                label: '已取件'\r\n            }],\r\n            options1: [{\r\n                value: 0,\r\n                label: '否',\r\n            }, {\r\n                value: 1,\r\n                label: '是',\r\n            }],\r\n            editDialogVisible: false,\r\n            form: {},\r\n\r\n        }\r\n    },\r\n    created() {\r\n        this.user=localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\r\n        this.searchOrdersByUsername()\r\n    },\r\n    methods:\r\n        {\r\n            //上传照片\r\n            handleAddPhoto(ordername){\r\n                this.centerDialogVisible = true\r\n                this.currentOrdername = ordername\r\n            },\r\n            updateData(){\r\n                this.searchOrdersByUsername()\r\n            },\r\n            deleteOrder(orderName){\r\n                try {\r\n                    this.request.delete(\"/order/ordername/\" + orderName).then(res => {\r\n                        if (res.code === '200'){\r\n                            this.$message.success(\"删除成功\")\r\n                            this.updateData()\r\n                        }\r\n                    })\r\n                } catch (e) {\r\n                    this.$message.error(\"删除失败\")\r\n                }\r\n            },\r\n            uploadFileError(err, file, fileList){\r\n                this.$message.error(\"上传失败！\")\r\n            },\r\n            exceedFile(files, fileList){\r\n                this.$message.error('只能上传'+this.limitnum+'个文件');\r\n            },\r\n            removeFile(file,fileList) {\r\n                this.fileList=fileList;\r\n            },\r\n            loadJsonFromFile(file, fileList) {\r\n                this.fileList = fileList\r\n            },\r\n            onBeforeUploadImage(file) {\r\n                const isIMAGE = file.type === 'image/jpeg' || 'image/jpg' || 'image/png'\r\n                const isLt1M = file.size / 1024 / 1024 < 5\r\n                if (!isIMAGE) {\r\n                    this.$message.error('上传文件只能是图片格式!')\r\n                }\r\n                if (!isLt1M) {\r\n                    this.$message.error('上传文件大小不能超过 1MB!')\r\n                }\r\n                return isIMAGE && isLt1M\r\n            },\r\n            uploadSuccess(response, file, fileList){\r\n                let order_file = {\r\n                    \"orderName\": this.currentOrdername,\r\n                    \"userName\": '',\r\n                    \"url\": response\r\n                }\r\n                try {\r\n                    this.request.post(\"/order-file\", order_file).then(res => {\r\n                        if (res.code === '200') {\r\n                            this.searchOrdersByUsername()\r\n                            this.$message.success(\"上传成功\")\r\n                        }\r\n                    })\r\n                    this.searchOrdersByUsername()\r\n                } catch (e){\r\n                    this.$message.error(\"上传失败\")\r\n                    this.request.delete(\"/file/url/\"+ response).then(res => {\r\n\r\n                    })\r\n                }\r\n            },\r\n            //加载表单\r\n            searchOrdersByUsername(){\r\n                this.request.get(\"/order/page\", {\r\n                    params: {\r\n                        pageNum: this.pageNum,\r\n                        pageSize: this.pageSize,\r\n                        username: this.username,\r\n                        name: this.name\r\n                    }\r\n                }).then(res => {\r\n                    res = JSON.parse(JSON.stringify(res.data))\r\n                    this.tableData = res.records\r\n                    this.pageNum = res.pages\r\n                    this.total = res.total\r\n                    console.log(res)\r\n                    console.log(this.tableData)\r\n                    const newProperty1='userUpload'\r\n                    const newProperty2='adminUpload'\r\n                    for (let i = 0; i < this.tableData.length; i++) {\r\n                        this.request.get(\"/order-file/ordername/\" + this.tableData[i].orderName).then(res => {\r\n                            this.tableData[i][newProperty1] = []\r\n                            this.tableData[i][newProperty2] = []\r\n                            if (res.data != null)\r\n                            {\r\n                                for(let k = 0; k < res.data.length; k++){\r\n                                    if (res.data[k].adminUp === false){\r\n                                        this.tableData[i][newProperty1].push(res.data[k].url)\r\n                                    } else {\r\n                                        this.tableData[i][newProperty2].push(res.data[k].url)\r\n                                    }\r\n                                }\r\n                            }\r\n                        })\r\n                        ///保险自提\r\n                        this.tableData[i].others = [this.tableData[i].others[0] + this.tableData[i].others[1], this.tableData[i].others[3] + this.tableData[i].others[4]]\r\n                        //是否确认\r\n                        if (this.tableData[i].isConfirmed === true ){\r\n                            this.tableData[i].isConfirmed = '是'\r\n                        } else {\r\n                            this.tableData[i].isConfirmed = '否'\r\n                        }\r\n                    }\r\n                })\r\n            },\r\n            showUserPhoto(urls) {\r\n                this.UserDialogVisible = true\r\n                this.urls = urls\r\n\r\n            },\r\n            showAdminPhoto(urls){\r\n                this.AdminDialogVisible = true\r\n                this.urls = urls\r\n            },\r\n            confirmeOrder(order){\r\n                order = {\r\n                    id: order.id,\r\n                    orderName: order.orderName,\r\n                    isConfirmed: true\r\n                }\r\n                console.log(order)\r\n                this.request.post(\"/order/update\", order).then(res => {\r\n                    if (res.code === '200'){\r\n                        this.searchOrdersByUsername()\r\n                        this.$message.success(\"保存成功\")\r\n                    }\r\n                    else {\r\n                        this.$message.error(res.msg)\r\n                    }\r\n                })\r\n\r\n            },\r\n            handleSizeChange(pageSize) {\r\n                this.pageSize = pageSize\r\n                this.searchOrdersByUsername()\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                this.pageNum = pageNum\r\n                this.searchOrdersByUsername()\r\n            },\r\n            reset(){\r\n                this.username=\"\"\r\n                this.name=\"\"\r\n                this.searchOrdersByUsername()\r\n            },\r\n            //编辑订单\r\n            editOrder(order){\r\n                this.editDialogVisible = true\r\n                this.form.id = order.id\r\n                this.form.orderName = order.orderName\r\n                this.form.isConfirmed = order.isConfirmed\r\n                this.form.oderStatus = order.oderStatus\r\n                this.form.email = order.email\r\n                this.form.phone = order.phone\r\n                this.form.receiveAdress = order.receiveAdress\r\n\r\n            },\r\n            updateOrder(){\r\n                console.log(this.form)\r\n                // this.request.post(\"/order/update\", this.form).then(res => {\r\n                //     if (res.code === '200') {\r\n                //         this.searchOrdersByUsername()\r\n                //         this.editDialogVisible = false\r\n                //         this.$message.success(\"上传成功\")\r\n                //\r\n                //     }\r\n                // })\r\n            }\r\n        }\r\n})\r\n</script>\r\n\r\n<template>\r\n    <div>\r\n        <div style=\"margin: 10px 0\">\r\n            <el-input style=\"width: 200px\" placeholder=\"按用户名搜索\" suffix-icon=\"el-icon-search\" v-model=\"username\"></el-input>\r\n            <el-input style=\"width: 200px\" placeholder=\"按用姓名搜索\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n            <el-button class=\"ml-5\" type=\"primary\" @click=\"searchOrdersByUsername\">搜索</el-button>\r\n            <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n        </div>\r\n\r\n        <el-table\r\n            header-cell-style=\"background-color: #DCDFE6; color: #606266;\"\r\n            :data=\"tableData\"\r\n            border\r\n            style=\"width: 100%; border-radius:25px \"\r\n            height=\"720\"\r\n            width=\"100%\"\r\n            size=\"medium\"\r\n            default-expand-all=\"ture\">\r\n            <el-table-column type=\"expand\" align-center=\"true\">\r\n                <template slot-scope=\"props\">\r\n\r\n                    <el-descriptions column=\"3\" size=\"mini\" border>\r\n                        <template slot=\"extra\">\r\n                            <el-button type=\"primary\" size=\"mini\" @click=\"editOrder(props.row)\">编辑订单</el-button>\r\n                            <el-button type=\"primary\" size=\"mini\" @click=\"confirmeOrder(props.row)\">确认订单</el-button>\r\n                            <el-button type=\"primary\" size=\"mini\" @click=\"showAdminPhoto(props.row.adminUpload)\">查看管理员上传</el-button>\r\n                            <el-button type=\"primary\" size=\"mini\" @click=\"showUserPhoto(props.row.userUpload)\">查看用户上传</el-button>\r\n                            <el-button type=\"primary\" size=\"mini\" @click=\"handleAddPhoto(props.row.orderName)\">上传照片</el-button>\r\n                            <el-button type=\"danger\" size=\"mini\" @click=\"deleteOrder(props.row.orderName)\">撤销订单</el-button>\r\n                        </template>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-user\"></i>\r\n                                姓名\r\n                            </template>\r\n                            {{ props.row.name }}\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-mobile-phone\"></i>\r\n                                手机号\r\n                            </template>\r\n                            {{ props.row.phone }}\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-location-outline\"></i>\r\n                                物品数量和重量\r\n                            </template>\r\n                            {{props.row.weight}}kg，{{ props.row.deliveryNum }}个\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-tickets\"></i>\r\n                                收货地址\r\n                            </template>\r\n                            <el-tag size=\"small\">{{ props.row.receiveAdress}}</el-tag>\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-office-building\"></i>\r\n                                寄货站点\r\n                            </template>\r\n                            {{props.row.sendRegion}}\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-office-building\"></i>\r\n                                取货站点\r\n                            </template>\r\n                            {{ props.row.receiveRegion}}\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-tickets\"></i>\r\n                                邮箱\r\n                            </template>\r\n                            <el-tag size=\"small\">{{ props.row.email}}</el-tag>\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-office-building\"></i>\r\n                                物品详情\r\n                            </template>\r\n                            {{ props.row.details}}\r\n                        </el-descriptions-item>\r\n                        <el-descriptions-item>\r\n                            <template slot=\"label\">\r\n                                <i class=\"el-icon-office-building\"></i>\r\n                                备注\r\n                            </template>\r\n                            {{ props.row.remark}}\r\n                        </el-descriptions-item>\r\n                    </el-descriptions>\r\n                    <template>\r\n                        <div style=\"margin-top: 10px\">\r\n                            <el-steps :space=\"200\" :active=\"props.row.oderStatus\" finish-status=\"success\" align-center=\"true\">\r\n                                <el-step title=\"订单已提交\"></el-step>\r\n                                <el-step title=\"订单等待付款\"></el-step>\r\n                                <el-step title=\"已发出\"></el-step>\r\n                                <el-step :title=\"'已到达' + props.row.receiveRegion\"></el-step>\r\n                                <el-step title=\"待取件\"></el-step>\r\n                                <el-step title=\"已取件\"></el-step>\r\n                            </el-steps>\r\n                        </div>\r\n                    </template>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column\r\n                label=\"用户名\"\r\n                prop=\"username\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                label=\"订单号\"\r\n                prop=\"orderName\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                label=\"保险\"\r\n                prop=\"others[0]\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                label=\"自提\"\r\n                prop=\"others[1]\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                label=\"创建时间\"\r\n                prop=\"createTime\">\r\n            </el-table-column>\r\n            <el-table-column\r\n                label=\"订单确认\"\r\n                prop=\"isConfirmed\">\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <div style=\"padding: 10px 0\">\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"pageNum\"\r\n                :page-sizes=\"[5, 10, 15, 20]\"\r\n                :page-size=\"pageSize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n\r\n        <el-dialog\r\n            title=\"上传照片\"\r\n            :visible.sync=\"centerDialogVisible\"\r\n            width=\"30%\"\r\n            center>\r\n            <el-upload\r\n                class=\"upload-demo\"\r\n                accept=\"image/jpeg,image/png,image/jpg\"\r\n                ref=\"upload\"\r\n                :auto-upload=\"true\"\r\n                :file-list=\"fileList\"\r\n                :before-upload=\"onBeforeUploadImage\"\r\n                :on-success=\"uploadSuccess\"\r\n                :on-change=\"loadJsonFromFile\"\r\n                :limit=\"limitnum\"\r\n                :on-exceed=\"exceedFile\"\r\n                :on-remove=\"removeFile\"\r\n                :on-error=\"uploadFileError\"\r\n                action=\"http://localhost:9090/file/upload\">\r\n                <el-button slot=\"trigger\" size=\"small\" type=\"primary\">选取照片</el-button>\r\n                <div slot=\"tip\" class=\"el-upload__tip\">只能上传jpg/png文件，且不超过500kb</div>\r\n            </el-upload>\r\n        </el-dialog>\r\n\r\n        <el-dialog\r\n            title=\"我的上传\"\r\n            :visible.sync=\"UserDialogVisible\"\r\n            width=\"50%\">\r\n            <div class=\"demo-image__lazy\">\r\n                <el-image v-for=\"url in urls\" :key=\"url\" :src=\"url\" lazy preview-src-list></el-image>\r\n            </div>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button type=\"primary\" @click=\"UserD