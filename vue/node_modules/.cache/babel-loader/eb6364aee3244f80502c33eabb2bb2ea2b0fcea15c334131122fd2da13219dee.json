{"ast":null,"code":"import Vue from 'vue';\nimport { Editor, Toolbar } from '@wangeditor/editor-for-vue';\nimport { serverIp, serverPort, http } from \"../../public/config\";\nexport default Vue.extend({\n  components: {\n    Editor,\n    Toolbar\n  },\n  data() {\n    return {\n      editor: null,\n      html: '<p>hello</p>',\n      toolbarConfig: {},\n      mode: 'default' // or 'simple'\n    };\n  },\n\n  methods: {\n    onCreated(editor) {\n      this.editor = Object.seal(editor); // 一定要用 Object.seal() ，否则会报错\n      this.editorConfig.MENU_CONF['uploadImage'] = {\n        server: http + \"://\" + serverIp + \":\" + serverPort + \"/file/upload\"\n      };\n    },\n    onChange(editor) {\n      console.log(editor.getHtml());\n    }\n  },\n  mounted() {\n    // 模拟 ajax 请求，异步渲染编辑器\n    setTimeout(() => {\n      this.html = '<p>模拟 Ajax 异步设置内容 HTML</p>';\n    }, 1500);\n  },\n  beforeDestroy() {\n    const editor = this.editor;\n    if (editor == null) return;\n    editor.destroy(); // 组件销毁时，及时销毁编辑器\n  }\n});","map":{"version":3,"names":["Vue","Editor","Toolbar","serverIp","serverPort","http","extend","components","data","editor","html","toolbarConfig","mode","methods","onCreated","Object","seal","editorConfig","MENU_CONF","server","onChange","console","log","getHtml","mounted","setTimeout","beforeDestroy","destroy"],"sources":["src/views/DeliveryCN.vue"],"sourcesContent":["<template>\r\n    <div style=\"border: 1px solid #ccc;\">\r\n        <Toolbar\r\n            style=\"border-bottom: 1px solid #ccc\"\r\n            :editor=\"editor\"\r\n            :defaultConfig=\"toolbarConfig\"\r\n            :mode=\"mode\"\r\n        />\r\n        <Editor\r\n            style=\"height: 500px; overflow-y: hidden;\"\r\n            v-model=\"html\"\r\n            :defaultConfig=\"editorConfig\"\r\n            :mode=\"mode\"\r\n            @onCreated=\"onCreated\"\r\n            @onChange=\"onChange\"\r\n        />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Vue from 'vue'\r\nimport { Editor, Toolbar } from '@wangeditor/editor-for-vue'\r\nimport {serverIp, serverPort,http} from \"../../public/config\";\r\nexport default Vue.extend({\r\n    components: { Editor, Toolbar },\r\n    \r\n    data() {\r\n        return {\r\n            editor: null,\r\n            html: '<p>hello</p>',\r\n            toolbarConfig: { },\r\n            mode: 'default', // or 'simple'\r\n        }\r\n    },\r\n    methods: {\r\n        onCreated(editor) {\r\n            this.editor = Object.seal(editor) // 一定要用 Object.seal() ，否则会报错\r\n            this.editorConfig.MENU_CONF['uploadImage'] = {\r\n                server: http+\"://\"+serverIp+\":\" + serverPort + \"/file/upload\"\r\n            }\r\n\r\n        },\r\n        onChange(editor){\r\n            console.log(editor.getHtml())\r\n        }\r\n    },\r\n    mounted() {\r\n\r\n        // 模拟 ajax 请求，异步渲染编辑器\r\n        setTimeout(() => {\r\n            this.html = '<p>模拟 Ajax 异步设置内容 HTML</p>'\r\n        }, 1500)\r\n    },\r\n    beforeDestroy() {\r\n        const editor = this.editor\r\n        if (editor == null) return\r\n        editor.destroy() // 组件销毁时，及时销毁编辑器\r\n    }\r\n})\r\n</script>\r\n\r\n\r\n<style src=\"@wangeditor/editor/dist/css/style.css\"></style>"],"mappings":"AAoBA,OAAAA,GAAA;AACA,SAAAC,MAAA,EAAAC,OAAA;AACA,SAAAC,QAAA,EAAAC,UAAA,EAAAC,IAAA;AACA,eAAAL,GAAA,CAAAM,MAAA;EACAC,UAAA;IAAAN,MAAA;IAAAC;EAAA;EAEAM,KAAA;IACA;MACAC,MAAA;MACAC,IAAA;MACAC,aAAA;MACAC,IAAA;IACA;EACA;;EACAC,OAAA;IACAC,UAAAL,MAAA;MACA,KAAAA,MAAA,GAAAM,MAAA,CAAAC,IAAA,CAAAP,MAAA;MACA,KAAAQ,YAAA,CAAAC,SAAA;QACAC,MAAA,EAAAd,IAAA,WAAAF,QAAA,SAAAC,UAAA;MACA;IAEA;IACAgB,SAAAX,MAAA;MACAY,OAAA,CAAAC,GAAA,CAAAb,MAAA,CAAAc,OAAA;IACA;EACA;EACAC,QAAA;IAEA;IACAC,UAAA;MACA,KAAAf,IAAA;IACA;EACA;EACAgB,cAAA;IACA,MAAAjB,MAAA,QAAAA,MAAA;IACA,IAAAA,MAAA;IACAA,MAAA,CAAAkB,OAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}