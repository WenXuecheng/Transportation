/utils/request\";\r\nimport router, {resetRouter} from \"@/router\";\r\nexport default defineComponent({\r\n    name: \"createorder\",\r\n    data() {\r\n        var validatestr = (rule, value, callback) => {\r\n            if(this.isValidString(value) == false){\r\n                // this.$refs.ruleForm.validateField('checkPass');\r\n                callback(new Error('Пожалуйста, введите правильно'));\r\n            } else {\r\n                callback();\r\n            }\r\n        };\r\n        var validatePhone = (rule, value, callback) => {\r\n            if(this.isValidStringPhone(value) == false){\r\n                // this.$refs.ruleForm.validateField('checkPass');\r\n                callback(new Error('Example：89692184441'));\r\n            } else {\r\n                callback();\r\n            }\r\n        };\r\n        return {\r\n            ruleForm: {\r\n                orderName: '',\r\n                sendRegion: '',\r\n                receiveRegion: '',\r\n                receiveAdress: '',\r\n                dateOut: '',\r\n                deliveryNum: 1,\r\n                others: [],\r\n                name: '',\r\n                phone: '',\r\n                email: '',\r\n                details: '',\r\n                remark: '',\r\n                pay: '',\r\n                username: '',\r\n            },\r\n            rules: {\r\n                sendRegion: [\r\n                    { required: true, message: 'Пожалуйста, выберите место доставки', trigger: 'change' },\r\n\r\n                ],\r\n                receiveRegion: [\r\n                    { required: true, message: 'Пожалуйста, выберите пункт самовывоза', trigger: 'change' }\r\n                ],\r\n                dateOut: [\r\n                    { type: 'date', required: true, message: 'Пожалуйста, выберите дату', trigger: 'change' }\r\n                ],\r\n                name: [\r\n                    { required: true, message: 'Пожалуйста, введите Ваше ФИО', trigger: 'blur' },\r\n                    { validator: validatestr,  trigger: 'blur'}\r\n                ],\r\n                phone: [\r\n                    { required: true, message: 'Пожалуйста, введите ваш номер телефона', trigger: 'blur' },\r\n                    { validator: validatePhone,  trigger: 'blur'}\r\n                ],\r\n                email: [\r\n                    { required: true, message: 'Пожалуйста, введите ваш адрес электронной почты', trigger: 'blur'},\r\n                    { validator: validatestr,  trigger: 'blur'}\r\n                ],\r\n                receiveAdress: [\r\n                    { required: true, message: 'Пожалуйста, заполните адрес доставки', trigger: 'blur' },\r\n                    { validator: validatestr,  trigger: 'blur'}\r\n                ],\r\n                details: [\r\n                    { required: true, message: 'Пожалуйста, заполните данные о товаре', trigger: 'blur' },\r\n                    { validator: validatestr,  trigger: 'blur'}\r\n                ],\r\n                remark: [\r\n                    { required: false, message: '请填写活动形式', trigger: 'blur' },\r\n                    { validator: validatestr,  trigger: 'blur'}\r\n                ],\r\n                pay: [\r\n                    { required: true, message: '请填选择支付方式', trigger: 'blur' },\r\n                    { validator: validatestr,  trigger: 'blur'}\r\n                ],\r\n            },\r\n            vMode: [\"1\",\"2\"] //打开折叠窗口\r\n        };\r\n    },\r\n    created() {\r\n    },\r\n    methods: {\r\n        //表格上传操作\r\n        submitForm(formName) {\r\n            this.updateuser()\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    this.setOthers()\r\n                    request.post(\"/order\", this.ruleForm).then(res => {\r\n                        if (res.code === '200') {\r\n                            this.open()\r\n                            this.resetForm('ruleForm')\r\n                        } else {\r\n                            this.$message.error(res.msg)\r\n                        }\r\n                    })\r\n                } else {\r\n                    return false;\r\n                }\r\n            });\r\n        },\r\n        //创建订单后的提醒\r\n        open() {\r\n            const h = this.$createElement;\r\n\r\n            this.$notify({\r\n                title: '订单创建成功',\r\n                message: h('i', { style: 'color: teal'}, '请注意查看邮箱')\r\n            });\r\n        },\r\n        isValidStringPhone(str) {\r\n            try {\r\n                const pattern = /^[0-9]+$/;\r\n                return pattern.test(str);\r\n            } catch (e){\r\n                return null\r\n            }\r\n        },\r\n        isValidString(str) {\r\n            try {\r\n                // 检查首个字符是否为空格\r\n                if (str !== '' && str[0] === ' ') {\r\n                    return false;\r\n                }\r\n                return true;\r\n            } catch (e){\r\n                return null\r\n            }\r\n        },\r\n        updateuser(){\r\n            try {\r\n                this.user=localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\r\n                if (this.user === null){\r\n                } else {\r\n                    this.ruleForm.username=this.user.username\r\n                }\r\n            } catch (e){\r\n                this.$store.commit(\"logout\")\r\n                this.$message.error(\"请从新登录\")\r\n            }\r\n        },\r\n        setOthers(){\r\n            if (this.ruleForm.others[0] == null)\r\n            {\r\n                this.ruleForm.others[0] = \"没有\"\r\n            }\r\n            if (this.ruleForm.others[1] == null)\r\n            {\r\n                this.ruleForm.others[1] = \"没有\"\r\n            }\r\n            this.ruleForm.others = this.ruleForm.others[0] + '/' + this.ruleForm.others[1]\r\n        },\r\n        resetForm(formName) {\r\n            this.$refs[formName].resetFields();\r\n        }\r\n    }\r\n})\r\n</script>\r\n\r\n<template>\r\n    <div>\r\n        <el-collapse v-model=\"vMode\">\r\n            <el-collapse-item title=\"下单流程\" name=\"1\">\r\n                <div><el-alert type=\"success\">填写订单表格=>电话转账或者到快递点付款=>收到下单的邮件=>将快件送到我们站点=>付款=>在首页查看订单状态</el-alert></div>\r\n                <div><el-alert type=\"success\">您可以选择将物品寄送到站点，然后您会收到付款邮件，并在邮件中付款</el-alert></div>\r\n                <div><el-alert type=\"success\">付款后订单的状态会改变</el-alert></div>\r\n            </el-collapse-item>\r\n            <el-collapse-item title=\"下单须知\" name=\"2\">\r\n                <div><el-alert type=\"error\">粉末类，易燃易爆类，液体类，烟膏，烟油，药品保健类拒收</el-alert></div>\r\n                <div><el-alert type=\"success\">一般时效12-15到达俄罗斯莫斯</el-alert></div>\r\n                <div><el-alert type=\"success\">订单号系统自动生成不需要填写</el-alert></div>\r\n                <div><el-alert type=\"warning\">下单后请及时上传照片</el-alert></div>\r\n            </el-collapse-item>\r\n        </el-collapse>\r\n\r\n        <el-card class=\"box-card\">\r\n            <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\">\r\n                <el-form-item label=\"订单号\" prop=\"orderName\">\r\n                    <el-input v-model=\"ruleForm.orderName\" disabled ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄货站点\" prop=\"sendRegion\" required>\r\n                    <el-select v-model=\"ruleForm.sendRegion\" placeholder=\"选择服务区\" style=\"width: 25%\">\r\n                        <el-option label=\"莫斯科\" value=\"莫斯科\"></el-option>\r\n                        <el-option label=\"广东\" value=\"广东\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"取货站点\" prop=\"receiveRegion\" required>\r\n                    <el-select v-model=\"ruleForm.receiveRegion\" placeholder=\"选择服务区\" style=\"width: 25%\">\r\n                        <el-option label=\"莫斯科\" value=\"莫斯科\"></el-option>\r\n                        <el-option label=\"广东\" value=\"广东\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄出时间\" required>\r\n                    <el-form-item prop=\"dateOut\">\r\n                        <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"ruleForm.dateOut\" style=\"width: 25%;\"></el-date-picker>\r\n                    </el-form-item>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"物品数量\" prop=\"deliveryNum\">\r\n                    <template>\r\n                        <template>\r\n                            <el-input-number v-model=\"ruleForm.deliveryNum\" :step=\"1\" step-strictly></el-input-number>\r\n                        </template>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"其他\" prop=\"others\">\r\n                    <el-checkbox-group v-model=\"ruleForm.others\">\r\n                        <el-checkbox label=\"自提\" name=\"type\"></el-checkbox>\r\n                        <el-checkbox label=\"保险\" name=\"type\"></el-checkbox>\r\n                    </el-checkbox-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"姓名\" prop=\"name\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.name\"\r\n                        show-word-limit\r\n                        style=\"width: 25%\"\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人电话\" prop=\"phone\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.phone\"\r\n                        show-word-limit\r\n                        style=\"width: 25%\"\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人邮箱\" prop=\"email\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.email\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"收货地址\" prop=\"receiveAdress\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.receiveAdress\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"物品详情\" prop=\"details\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.details\" maxlength=\"50\"\r\n                              show-word-limit></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"备注\" prop=\"remark\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.remark\" maxlength=\"50\"\r\n                              show-word-limit></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"支付方式\" prop=\"pay\">\r\n                <el-row>\r\n                    <el-col :span=\"10\"><div>\r\n                        <el-radio-group v-model=\"ruleForm.pay\">\r\n                            <el-radio-button label=\"到店付款\"></el-radio-button>\r\n                            <el-radio-button label=\"在线转账\"></el-radio-button>\r\n                        </el-radio-group>\r\n                    </div></el-col>\r\n\r\n                    <el-col :span=\"24\"><div class=