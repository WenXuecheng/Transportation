{"ast":null,"code":"import { defineComponent } from 'vue';\nimport request from \"@/utils/request\";\nimport router, { resetRouter } from \"@/router\";\nexport default defineComponent({\n  name: \"createorder\",\n  data() {\n    var validatestr = (rule, value, callback) => {\n      if (this.isValidString(value) == false) {\n        // this.$refs.ruleForm.validateField('checkPass');\n        callback(new Error('Пожалуйста, введите правильно'));\n      } else {\n        callback();\n      }\n    };\n    var validatePhone = (rule, value, callback) => {\n      if (this.isValidStringPhone(value) == false) {\n        // this.$refs.ruleForm.validateField('checkPass');\n        callback(new Error('Example：89692184441'));\n      } else {\n        callback();\n      }\n    };\n    return {\n      ruleForm: {\n        orderName: '',\n        sendRegion: '',\n        receiveRegion: '',\n        receiveAdress: '',\n        dateOut: '',\n        deliveryNum: 1,\n        others: [],\n        name: '',\n        phone: '',\n        email: '',\n        details: '',\n        remark: '',\n        pay: '',\n        username: ''\n      },\n      rules: {\n        sendRegion: [{\n          required: true,\n          message: 'Пожалуйста, выберите место доставки',\n          trigger: 'change'\n        }],\n        receiveRegion: [{\n          required: true,\n          message: 'Пожалуйста, выберите пункт самовывоза',\n          trigger: 'change'\n        }],\n        dateOut: [{\n          type: 'date',\n          required: true,\n          message: 'Пожалуйста, выберите дату',\n          trigger: 'change'\n        }],\n        name: [{\n          required: true,\n          message: 'Пожалуйста, введите Ваше ФИО',\n          trigger: 'blur'\n        }, {\n          validator: validatestr,\n          trigger: 'blur'\n        }],\n        phone: [{\n          required: true,\n          message: 'Пожалуйста, введите ваш номер телефона',\n          trigger: 'blur'\n        }, {\n          validator: validatePhone,\n          trigger: 'blur'\n        }],\n        email: [{\n          required: true,\n          message: 'Пожалуйста, введите ваш адрес электронной почты',\n          trigger: 'blur'\n        }, {\n          validator: validatestr,\n          trigger: 'blur'\n        }],\n        receiveAdress: [{\n          required: true,\n          message: 'Пожалуйста, заполните адрес доставки',\n          trigger: 'blur'\n        }, {\n          validator: validatestr,\n          trigger: 'blur'\n        }],\n        details: [{\n          required: true,\n          message: 'Пожалуйста, заполните данные о товаре',\n          trigger: 'blur'\n        }, {\n          validator: validatestr,\n          trigger: 'blur'\n        }],\n        remark: [{\n          required: false,\n          message: '请填写活动形式',\n          trigger: 'blur'\n        }, {\n          validator: validatestr,\n          trigger: 'blur'\n        }],\n        pay: [{\n          required: true,\n          message: '请填选择支付方式',\n          trigger: 'blur'\n        }, {\n          validator: validatestr,\n          trigger: 'blur'\n        }]\n      },\n      vMode: [\"1\", \"2\"] //打开折叠窗口\n    };\n  },\n\n  created() {},\n  methods: {\n    //表格上传操作\n    submitForm(formName) {\n      this.updateuser();\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          this.setOthers();\n          request.post(\"/order\", this.ruleForm).then(res => {\n            if (res.code === '200') {\n              this.open();\n              this.resetForm('ruleForm');\n            } else {\n              this.$message.error(res.msg);\n            }\n          });\n        } else {\n          return false;\n        }\n      });\n    },\n    //创建订单后的提醒\n    open() {\n      const h = this.$createElement;\n      this.$notify({\n        title: '订单创建成功',\n        message: h('i', {\n          style: 'color: teal'\n        }, '请注意查看邮箱')\n      });\n    },\n    isValidStringPhone(str) {\n      t