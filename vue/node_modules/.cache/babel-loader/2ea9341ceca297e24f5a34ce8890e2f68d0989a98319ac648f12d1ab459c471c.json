{"ast":null,"code":"export default {\n  name: \"FrontHome\",\n  props: {\n    user: Object\n  },\n  data() {\n    return {\n      exchangeRate: null,\n      usrmbRates: null,\n      photoandannouncement: {\n        photo1: 'https://www.ups.com/assets/resources/supplychain/images/350x197/m15-350x197-fast-fulfillment-assembly.jpg',\n        photo23: ['https://www.ups.com/assets/resources/supplychain/images/350x197/m15-350x197-fast-fulfillment-assembly.jpg'],\n        announcement1: {\n          p1: \"None\",\n          p2: \"None\"\n        },\n        announcement2: {\n          title: \"None\",\n          p1: \"None\",\n          p2: \"None\"\n        },\n        announcement3: {\n          title: \"None\",\n          p1: \"None\",\n          p2: \"None\"\n        }\n      },\n      order: {\n        \"id\": \"\",\n        \"orderName\": \"\",\n        \"sendRegion\": \"\",\n        \"receiveRegion\": \"\",\n        \"receiveAdress\": \"\",\n        \"dateOut\": \"\",\n        \"deliveryNum\": \"\",\n        \"others\": [],\n        \"name\": \"\",\n        \"phone\": \"\",\n        \"email\": \"\",\n        \"details\": \"\",\n        \"remark\": \"\",\n        \"username\": \"\",\n        \"weight\": \"\",\n        \"orderStatus\": \"\"\n      },\n      tableData: [],\n      dialogVisible: false\n    };\n  },\n  created() {\n    this.editorConfig = {\n      // 访问 UEditor 静态资源的根路径，可参考常见问题1\n      UEDITOR_HOME_URL: '/UEditor/',\n      // 服务端接口（这个地址是我为了方便各位体验文件上传功能搭建的临时接口，请勿在生产环境使用！！！）\n      serverUrl: '//ueditor.szcloudplus.com/cos'\n    };\n  },\n  setup() {\n    const {\n      proxy\n    } = getCurrentInstance();\n    const state = reactive({\n      content: '你好',\n      editorConfig: {},\n      shopAddForm: {}\n    });\n    // 表单提交\n    const submitForm = () => {\n      const shopInfo = {};\n      shopInfo.detail = state.content;\n      shopInfo.title = '博客发表';\n      console.log(shopInfo);\n      proxy.$api.addBlob(shopInfo).then(({\n        data\n      }) => {\n        if (!data.code) {\n          console.log('新增成功');\n        }\n      });\n    };\n    return {\n      ...toRefs(state),\n      submitForm\n    };\n  },\n  mounted() {\n    const loading = this.$loading({\n      lock: true,\n      text: 'Loading',\n      spinner: 'el-icon-loading',\n      background: 'rgba(0, 0, 0, 0.8)'\n    });\n    setTimeout(() => {\n      loading.close();\n    }, 10000);\n    this.searchOrdersByUsername();\n    this.updateExchangeRate();\n    this.updateUsrmbRate();\n    this.request.get(\"/others/getphotoandannouncement\").then(res => {\n      if (res.code === '200') {\n        let data = res.data;\n        this.photoandannouncement.photo1 = data.photo1;\n        this.photoandannouncement.photo23[0] = data.photo2;\n        this.photoandannouncement.photo23[1] = data.photo3;\n        this.photoandannouncement.announcement1 = JSON.parse(data.announcement1);\n        this.photoandannouncement.announcement2 = JSON.parse(data.announcement2);\n        this.photoandannouncement.announcement3 = JSON.parse(data.announcement3);\n        loading.close();\n      }\n    });\n  },\n  methods: {\n    searchOrdersByUsername() {\n      this.request.get(\"/order/username\").then(res => {\n        try {\n          this.tableData = JSON.parse(JSON.stringify(res.data));\n          if (this.tableData.length === 0) {\n            return;\n          }\n          let j = 0;\n          let n = 0;\n          for (let i = 0; i < this.tableData.length; i++) {\n            if (this.tableData[i].id > n) {\n              j = i;\n              n = this.tableData[i].id;\n            }\n          }\n\n          ///others\n          this.tableData[j].others = this.tableData[j].others.split(\"/\");\n          console.log(this.tableData[j]);\n          let others = \"\";\n          for (let k = 0; k < this.tableData[j].others.length; k++) {\n            if (this.tableData[j].others[k] === \"保险\") {\n              others = others + this.$t(\"true\");\n            } else if (this.tableData[j].others[k] === \"自取\") {\n              others = others + this.$t(\"true\");\n            } else if (this.tableData[j].others[k] === \"木架\") {\n              others = others + this.$t(\"true\");\n            } else if (this.tableData[j].others[k] === \"防水\") {\n              others = others + this.$t(\"true\");\n            } else if (this.tableData[j].others[k] === \"托盘\") {\n              others = others + this.$t(\"true\");\n            } else {\n              others = others + this.$t(\"false\");\n            }\n            if (k !== this.tableData[j].others.length - 1) {\n              others = others + \"/\";\n            }\n          }\n          this.tableData[j].others = others;\n          console.log(this.tableData[j]);\n          //城市\n          switch (this.tableData[j].sendRegion) {\n            case \"Moscow\":\n              this.tableData[j].sendRegion = this.$t(\"Moscow\");\n              break;\n            case \"China\":\n              this.tableData[j].sendRegion = this.$t(\"China\");\n              break;\n            default:\n          }\n          switch (this.tableData[j].receiveRegion) {\n            case \"Moscow\":\n              this.tableData[j].receiveRegion = this.$t(\"Moscow\");\n              break;\n            case \"China\":\n              this.tableData[j].receiveRegion = this.$t(\"China\");\n              break;\n            default:\n          }\n          ///保险自提\n          this.tableData[j].others = [this.tableData[j].others[0] + this.tableData[j].others[1], this.tableData[j].others[3] + this.tableData[j].others[4]];\n          if (this.tableData[j].others[0] === \"保险\") {\n            this.tableData[j].others[0] = this.$t(\"true\");\n          } else {\n            this.tableData[j].others[0] = this.$t(\"false\");\n          }\n          if (this.tableData[j].others[1] === \"自取\") {\n            this.tableData[j].others[1] = this.$t(\"true\");\n          } else {\n            this.tableData[j].others[1] = this.$t(\"false\");\n          }\n          //是否确认\n          if (this.tableData[j].isConfirmed === true) {\n            this.tableData[j].isConfirmed = this.$t(this.tableData[j].isConfirmed.toString());\n          } else {\n            this.tableData[j].isConfirmed = this.$t(this.tableData[j].isConfirmed.toString());\n          }\n          this.order = this.tableData[j];\n          this.order.others = others;\n        } catch (e) {}\n      });\n    },\n    updateExchangeRate() {\n      this.request.get(\"/others/1\").then(res => {\n        if (res.code === '200') {\n          this.exchangeRate = res.data;\n        }\n      });\n    },\n    updateUsrmbRate() {\n      this.request.get(\"/others/1/UsRmbGet/1\").then(res => {\n        if (res.code === '200') {\n          this.usrmbRates = res.data;\n        }\n      });\n    },\n    getphotoandannouncement() {\n      this.request.get(\"/others/getphotoandannouncement\").then(res => {\n        if (res.code === '200') {\n          let data = res.data;\n          this.photoandannouncement.photo1 = data.photo1;\n          this.photoandannouncement.photo23[0] = data.photo2;\n          this.photoandannouncement.photo23[1] = data.photo3;\n          this.photoandannouncement.announcement1 = JSON.parse(data.announcement1);\n          this.photoandannouncement.announcement2 = JSON.parse(data.announcement2);\n          this.photoandannouncement.announcement3 = JSON.parse(data.announcement3);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","props","user","Object","data","exchangeRate","usrmbRates","photoandannouncement","photo1","photo23","announcement1","p1","p2","announcement2","title","announcement3","order","tableData","dialogVisible","created","editorConfig","UEDITOR_HOME_URL","serverUrl","setup","proxy","getCurrentInstance","state","reactive","content","shopAddForm","submitForm","shopInfo","detail","console","log","$api","addBlob","then","code","toRefs","mounted","loading","$loading","lock","text","spinner","background","setTimeout","close","searchOrdersByUsername","updateExchangeRate","updateUsrmbRate","request","get","res","photo2","photo3","JSON","parse","methods","stringify","length","j","n","i","id","others","split","k","$t","sendRegion","receiveRegion","isConfirmed","toString","e","getphotoandannouncement"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <template>\r\n            <div >\r\n                <el-row :gutter=\"20\">\r\n                    <el-col  :xs=\"24\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n                        <div style=\"height: 250px\">\r\n                            <el-card class=\"box-card\">\r\n                                <div style=\"height: 5%\">\r\n                                    <el-statistic group-separator=\",\" :precision=\"2\" decimal-separator=\".\" :value=\"exchangeRate\" :title=\"$t('exchangeRate')\">\r\n                                        <template slot=\"prefix\">\r\n                                            <i class=\"el-icon-s-flag\" style=\"color: red\"></i>\r\n                                        </template>\r\n                                        <template slot=\"suffix\">\r\n                                            <i class=\"el-icon-s-flag\" style=\"color: blue\"></i>\r\n                                        </template>\r\n                                    </el-statistic>\r\n                                    <el-statistic group-separator=\",\" :precision=\"2\" decimal-separator=\".\" :value=\"usrmbRates\" :title=\"$t('usrmbRate')\">\r\n                                        <template slot=\"prefix\">\r\n                                            <i class=\"el-icon-s-flag\" style=\"color: red\"></i>\r\n                                        </template>\r\n                                        <template slot=\"suffix\">\r\n                                            <i class=\"el-icon-s-flag\" style=\"color: blue\"></i>\r\n                                        </template>\r\n                                    </el-statistic>\r\n                                </div>\r\n                                <div style=\"height: 95%\">\r\n                                    <p>{{photoandannouncement.announcement1.p1}}</p>\r\n                                    <p>{{photoandannouncement.announcement1.p2}}</p>\r\n                                </div>\r\n                            </el-card>\r\n                        </div>\r\n                    </el-col>\r\n                    <el-col  :xs=\"24\" :sm=\"9\" :md=\"9\" :lg=\"9\" :xl=\"9\">\r\n                        <div style=\"height: 250px\">\r\n                            <el-card class=\"box-card\">\r\n                                <div style=\"margin-top: 1%;margin-left: 40%; color: #606266;height: 20px\">\r\n                                    <span>{{ $t(\"newgoods\") }}</span>\r\n                                </div>\r\n                                <div style=\"height: 250px\">\r\n                                        <el-image\r\n                                            style=\"width: 80%; height: 80%; margin-top: 5%;margin-left: 10%;\"\r\n                                            align=\"center\"\r\n                                            :src=\"photoandannouncement.photo1\"\r\n                                            fit=\"contain\">\r\n                                        </el-image>\r\n                                </div>\r\n                            </el-card>\r\n                        </div>\r\n                    </el-col>\r\n                    <el-col  :xs=\"24\" :sm=\"9\" :md=\"9\" :lg=\"9\" :xl=\"9\">\r\n                        <el-card class=\"box-card\">\r\n                            <template>\r\n                                <div style=\"margin-top: 1%;margin-left: 50%; color: #606266;height: 20px\">\r\n                                    <span>{{ $t(\"taobaodewu\") }}</span>\r\n                                </div>\r\n                                <el-carousel :interval=\"4000\" type=\"card\" height=\"220px\">\r\n                                    <el-carousel-item v-for=\"item in photoandannouncement.photo23\" :key=\"item\">\r\n                                        <el-image\r\n                                            style=\"width: 80%; height: 80%; margin-top: 5%;margin-left: 10%;\"\r\n                                            align=\"center\"\r\n                                            :src=\"item\"\r\n                                            fit=\"contain\">\r\n                                        </el-image>\r\n                                    </el-carousel-item>\r\n                                </el-carousel>\r\n                            </template>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n                <el-row :gutter=\"20\" style=\"margin-top: 10px\">\r\n                    <el-col  :xs=\"24\" :sm=\"12\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n                        <el-card class=\"box-card\" >\r\n                            <h3 style=\"text-align: center\">{{photoandannouncement.announcement2.title}}</h3>\r\n                            <p>{{photoandannouncement.announcement2.p1}}</p>\r\n                            <p>{{photoandannouncement.announcement2.p2}}</p>\r\n                        </el-card>\r\n                    </el-col>\r\n                    <el-col  :xs=\"24\" :sm=\"12\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n                        <el-card class=\"box-card\">\r\n                            <h3 style=\"text-align: center\">{{photoandannouncement.announcement3.title}}</h3>\r\n                            <p>{{photoandannouncement.announcement3.p1}}</p>\r\n                            <p>{{photoandannouncement.announcement3.p2}}</p>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n            </div>\r\n        </template>\r\n\r\n        <div class=\"bottom_box\">\r\n            <el-form :model=\"shopAddForm\" :inline=\"true\"\r\n                     label-width=\"160px\" label-position=\"left\" class=\"form_style\">\r\n                <el-form-item label=\"新增博客\" prop=\"content\">\r\n                    <vue-ueditor-wrap v-model=\"content\"\r\n                                      :config=\"editorConfig\"\r\n                                      editor-id=\"editor-demo-01\"></vue-ueditor-wrap>\r\n                </el-form-item>\r\n                <div style=\"margin: 0 0 16px 424px\">\r\n                    <el-button @click=\"$router.back(-1)\">取消</el-button>\r\n                    <el-button type=\"primary\" style=\"margin-left: 24px\"\r\n                               @click=\"submitForm\">保存</el-button>\r\n                </div>\r\n            </el-form>\r\n        </div>\r\n\r\n        <template>\r\n            <div style=\"margin-top: 1%\">\r\n                <template>\r\n                <el-descriptions column=\"3\" size=\"mini\" border >\r\n                    <el-descriptions-item  fixed>\r\n                        <template slot=\"label\" >\r\n                            <i class=\"el-icon-user\"></i>\r\n                            {{ $t(\"orderName\") }}\r\n                        </template>\r\n                        {{this.order.orderName}}\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-user\"></i>\r\n                            {{ $t(\"name\") }}\r\n                        </template>\r\n                        {{this.order.name}}\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-mobile-phone\"></i>\r\n                            {{ $t(\"phone\") }}\r\n                        </template>\r\n                        {{this.order.phone}}\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-location-outline\"></i>\r\n                            {{ $t(\"deliveryNum\") }}\r\n                        </template>\r\n                        {{this.order.deliveryNum}}\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item >\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-user\"></i>\r\n                            {{ $t(\"others.mode1\") }}\r\n                        </template>\r\n                        {{this.order.others}}\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-tickets\"></i>\r\n                            {{ $t(\"receiveAdress\") }}\r\n                        </template>\r\n                        <el-tag size=\"small\">{{this.order.receiveAdress}}</el-tag>\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-tickets\"></i>\r\n                            {{ $t(\"email\") }}\r\n                        </template>\r\n                        <el-tag size=\"small\">{{this.order.email}}</el-tag>\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-office-building\"></i>\r\n                            {{ $t('isConfirmed') }}\r\n                        </template>\r\n                        {{this.order.isConfirmed}}\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-office-building\"></i>\r\n                            {{ $t(\"details\") }}\r\n                        </template>\r\n                        {{this.order.details}}\r\n                    </el-descriptions-item>\r\n                    <el-descriptions-item>\r\n                        <template slot=\"label\">\r\n                            <i class=\"el-icon-office-building\"></i>\r\n                            {{ $t('remark') }}\r\n                        </template>\r\n                        {{this.order.remark}}\r\n                    </el-descriptions-item>\r\n                </el-descriptions>\r\n                </template>\r\n            </div>\r\n        </template>\r\n\r\n        <template>\r\n            <div style=\"margin-top: 10px\">\r\n                <el-steps :space=\"180\" :active=\"this.order.orderStatus\" finish-status=\"success\" align-center=\"true\">\r\n                    <el-step :title=\"$t('state1')\"></el-step>\r\n                    <el-step :title=\"$t('state3') + this.order.sendRegion+$t('state4')\"></el-step>\r\n                    <el-step :title=\"$t('state5') + this.order.receiveRegion+$t('state6')\"></el-step>\r\n                    <el-step :title=\"$t('state2')\"></el-step>\r\n                    <el-step :title=\"$t('state7')\"></el-step>\r\n                    <el-step :title=\"$t('state8')\"></el-step>\r\n                </el-steps>\r\n            </div>\r\n        </template>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"FrontHome\",\r\n    props: {\r\n        user: Object\r\n    },\r\n    data() {\r\n        return {\r\n            exchangeRate: null,\r\n            usrmbRates: null,\r\n            photoandannouncement: {\r\n                photo1: 'https://www.ups.com/assets/resources/supplychain/images/350x197/m15-350x197-fast-fulfillment-assembly.jpg',\r\n                photo23: ['https://www.ups.com/assets/resources/supplychain/images/350x197/m15-350x197-fast-fulfillment-assembly.jpg'],\r\n                announcement1: {p1:\"None\",p2:\"None\"},\r\n                announcement2: {title: \"None\", p1:\"None\", p2:\"None\"},\r\n                announcement3: {title: \"None\", p1:\"None\", p2:\"None\"}\r\n            },\r\n            order: {\r\n                \"id\":\"\",\r\n                \"orderName\":\"\",\r\n                \"sendRegion\":\"\",\r\n                \"receiveRegion\":\"\",\r\n                \"receiveAdress\":\"\",\r\n                \"dateOut\":\"\",\r\n                \"deliveryNum\":\"\",\r\n                \"others\":[],\r\n                \"name\":\"\",\r\n                \"phone\":\"\",\r\n                \"email\":\"\",\r\n                \"details\":\"\",\r\n                \"remark\":\"\",\r\n                \"username\":\"\",\r\n                \"weight\":\"\",\r\n                \"orderStatus\":\"\",\r\n            },\r\n            tableData: [],\r\n            dialogVisible: false,\r\n        }\r\n    },\r\n    created() {\r\n        this.editorConfig = {\r\n            // 访问 UEditor 静态资源的根路径，可参考常见问题1\r\n            UEDITOR_HOME_URL: '/UEditor/',\r\n            // 服务端接口（这个地址是我为了方便各位体验文件上传功能搭建的临时接口，请勿在生产环境使用！！！）\r\n            serverUrl: '//ueditor.szcloudplus.com/cos',\r\n        };\r\n\r\n    },\r\n    setup() {\r\n        const { proxy } = getCurrentInstance();\r\n        const state = reactive({\r\n            content: '你好',\r\n            editorConfig: {},\r\n            shopAddForm: {\r\n            },\r\n        });\r\n        // 表单提交\r\n        const submitForm = () => {\r\n            const shopInfo = {};\r\n            shopInfo.detail = state.content;\r\n            shopInfo.title = '博客发表';\r\n            console.log(shopInfo);\r\n            proxy.$api.addBlob(shopInfo).then(({ data }) => {\r\n                if (!data.code) {\r\n           