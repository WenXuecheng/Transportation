Name) {\n      this.$refs[formName].validate(valid => {\n        if (valid) {\n          console.log(this.ruleForm);\n          this.open();\n          this.resetForm();\n        } else {\n          console.log('error submit!!');\n          return false;\n        }\n      });\n    },\n    //表格重置操作\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    },\n    //创建订单后的提醒\n    open() {\n      const h = this.$createElement;\n      this.$notify({\n        title: '订单创建成功',\n        message: h('i', {\n          style: 'color: teal'\n        }, '请注意查看邮箱')\n      });\n    }\n  }\n});","map":{"version":3,"names":["defineComponent","name","data","ruleForm","orderName","sendRegion","dateOut","deliveryNum","others","phone","email","receiveRegion","details","remark","pay","rules","required","message","trigger","type","vMode","created","methods","submitForm","formName","$refs","validate","valid","console","log","open","resetForm","resetFields","h","$createElement","$notify","title","style"],"sources":["src/views/front/Createorder.vue"],"sourcesContent":["<script>\r\nimport {defineComponent} from 'vue'\r\n\r\nexport default defineComponent({\r\n    name: \"createorder\",\r\n    data() {\r\n        return {\r\n            ruleForm: {\r\n                orderName: '',\r\n                sendRegion: '',\r\n                dateOut: '',\r\n                deliveryNum: 0,\r\n                others: [],\r\n                name: '',\r\n                phone: '',\r\n                email: '',\r\n                receiveRegion: '',\r\n                details: '',\r\n                remark: '',\r\n                pay: '',\r\n\r\n            },\r\n            rules: {\r\n                sendRegion: [\r\n                    { required: true, message: '请选择地址', trigger: 'change' },\r\n                ],\r\n                dateOut: [\r\n                    { type: 'date', required: true, message: '请选择日期', trigger: 'change' }\r\n                ],\r\n                deliveryNum: [\r\n                    { type: 'number', required: true, message: '请输入数量', trigger: 'blur' }\r\n                ],\r\n                others: [\r\n                    { type: 'date', required: true, message: '请选择时间', trigger: 'change' }\r\n                ],\r\n                name: [\r\n                    { type: 'array', required: true, message: '请至少选择一个活动性质', trigger: 'change' }\r\n                ],\r\n                phone: [\r\n                    { required: true, message: '请选择活动资源', trigger: 'change' }\r\n                ],\r\n                email: [\r\n                    { required: true, message: '请填写活动形式', trigger: 'blur' }\r\n                ],\r\n                receiveRegion: [\r\n                    { required: true, message: '请填写活动形式', trigger: 'blur' }\r\n                ],\r\n                details: [\r\n                    { required: true, message: '请填写活动形式', trigger: 'blur' }\r\n                ],\r\n                remark: [\r\n                    { required: true, message: '请填写活动形式', trigger: 'blur' }\r\n                ],\r\n                pay: [\r\n                    { required: true, message: '请填写活动形式', trigger: 'blur' }\r\n                ],\r\n            },\r\n            vMode: [\"1\",\"2\"] //打开折叠窗口\r\n        };\r\n    },\r\n    created() {\r\n\r\n    },\r\n    methods: {\r\n        //表格上传操作\r\n        submitForm(formName) {\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    console.log(this.ruleForm)\r\n                    this.open()\r\n                    this.resetForm()\r\n                } else {\r\n                    console.log('error submit!!');\r\n                    return false;\r\n                }\r\n            });\r\n        },\r\n        //表格重置操作\r\n        resetForm(formName) {\r\n            this.$refs[formName].resetFields();\r\n        },\r\n        //创建订单后的提醒\r\n        open() {\r\n            const h = this.$createElement;\r\n\r\n            this.$notify({\r\n                title: '订单创建成功',\r\n                message: h('i', { style: 'color: teal'}, '请注意查看邮箱')\r\n            });\r\n        },\r\n    }\r\n})\r\n</script>\r\n\r\n<template>\r\n    <div>\r\n        <el-collapse v-model=\"vMode\">\r\n            <el-collapse-item title=\"下单流程\" name=\"1\">\r\n                <div><el-alert type=\"success\">填写订单表格=>电话转账或者到快递点付款=>收到下单的邮件=>将快件送到我们站点=>付款=>在首页查看订单状态</el-alert></div>\r\n                <div><el-alert type=\"success\">您可以选择将物品寄送到站点，然后您会收到付款邮件，并在邮件中付款</el-alert></div>\r\n                <div><el-alert type=\"success\">付款后订单的状态会改变</el-alert></div>\r\n            </el-collapse-item>\r\n            <el-collapse-item title=\"下单须知\" name=\"2\">\r\n                <div><el-alert type=\"error\">粉末类，易燃易爆类，液体类，烟膏，烟油，药品保健类拒收</el-alert></div>\r\n                <div><el-alert type=\"success\">一般时效12-15到达俄罗斯莫斯</el-alert></div>\r\n                <div><el-alert type=\"success\">订单号系统自动生成不需要填写</el-alert></div>\r\n                <div><el-alert type=\"warning\">下单后请及时上传照片</el-alert></div>\r\n            </el-collapse-item>\r\n        </el-collapse>\r\n\r\n        <el-card class=\"box-card\">\r\n            <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\">\r\n                <el-form-item label=\"订单号\" prop=\"orderName\">\r\n                    <el-input v-model=\"ruleForm.orderName\" disabled ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄货地址\" prop=\"sendRegion\" required>\r\n                    <el-select v-model=\"ruleForm.sendRegion\" placeholder=\"选择服务区\" style=\"width: 25%\">\r\n                        <el-option label=\"圣彼得堡\" value=\"stp\"></el-option>\r\n                        <el-option label=\"莫斯科\" value=\"moscow\"></el-option>\r\n                        <el-option label=\"广东\" value=\"guangDong\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄出时间\" required>\r\n                    <el-form-item prop=\"dateOut\">\r\n                        <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"ruleForm.dateOut\" style=\"width: 25%;\"></el-date-picker>\r\n                    </el-form-item>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"寄件数量\" prop=\"deliveryNum\">\r\n                    <template>\r\n                        <template>\r\n                            <el-input-number v-model=\"ruleForm.deliveryNum\" :step=\"1\" step-strictly></el-input-number>\r\n                        </template>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"其他\" prop=\"others\">\r\n                    <el-radio-group v-model=\"ruleForm.others\">\r\n                        <el-radio label=\"自提\"></el-radio>\r\n                        <el-radio label=\"保险\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"姓名\" prop=\"name\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.name\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人电话\" prop=\"phone\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.phone\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人邮箱\" prop=\"email\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.email\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"收货地址\" prop=\"receiveRegion\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.receiveRegion\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"物品详情\" prop=\"details\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.details\" maxlength=\"50\"\r\n                              show-word-limit></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"备注\" prop=\"remark\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.remark\" maxlength=\"50\"\r\n                              show-word-limit></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"支付方式\" prop=\"pay\">\r\n                <el-row>\r\n                    <el-col :span=\"10\"><div>\r\n                        <el-radio-group v-model=\"ruleForm.pay\">\r\n                            <el-radio-button label=\"到店付款\"></el-radio-button>\r\n                            <el-radio-button label=\"在线转账\"></el-radio-button>\r\n                        </el-radio-group>\r\n                    </div></el-col>\r\n\r\n                    <el-col :span=\"24\"><div class=\"grid-content bg-purple-dark\">\r\n                        <el-tag  type=\"success\" size=\"medium \">通过手机转账12348734857</el-tag>\r\n                    </div></el-col>\r\n                </el-row>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">立即创建</el-button>\r\n                    <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-card>\r\n\r\n\r\n<!--消息提醒-->\r\n        <template>\r\n            <el-button\r\n                plain\r\n                @click=\"open\">\r\n                可自动关闭\r\n            </el-button>\r\n        </template>\r\n    </div>\r\n</template>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AACA,SAAAA,eAAA;AAEA,eAAAA,eAAA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,SAAA;QACAC,UAAA;QACAC,OAAA;QACAC,WAAA;QACAC,MAAA;QACAP,IAAA;QACAQ,KAAA;QACAC,KAAA;QACAC,aAAA;QACAC,OAAA;QACAC,MAAA;QACAC,GAAA;MAEA;MACAC,KAAA;QACAV,UAAA,GACA;UAAAW,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAZ,OAAA,GACA;UAAAa,IAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAX,WAAA,GACA;UAAAY,IAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAV,MAAA,GACA;UAAAW,IAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAjB,IAAA,GACA;UAAAkB,IAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAT,KAAA,GACA;UAAAO,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAR,KAAA,GACA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAP,aAAA,GACA;UAAAK,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAN,OAAA,GACA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAL,MAAA,GACA;UAAAG,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAJ,GAAA,GACA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;MACAE,KAAA;IACA;EACA;;EACAC,QAAA,GAEA;EACAC,OAAA;IACA;IACAC,WAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAC,OAAA,CAAAC,GAAA,MAAA1B,QAAA;UACA,KAAA2B,IAAA;UACA,KAAAC,SAAA;QACA;UACAH,OAAA,CAAAC,GAAA;UACA;QACA;MACA;IACA;IACA;IACAE,UAAAP,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAQ,WAAA;IACA;IACA;IACAF,KAAA;MACA,MAAAG,CAAA,QAAAC,cAAA;MAEA,KAAAC,OAAA;QACAC,KAAA;QACAnB,OAAA,EAAAgB,CAAA;UAAAI,KAAA;QAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}