[\"1\",\"2\"]\r\n        };\r\n    },\r\n    created() {\r\n\r\n    },\r\n    methods: {\r\n        submitForm(formName) {\r\n            this.$refs[formName].validate((valid) => {\r\n                if (valid) {\r\n                    alert('submit!');\r\n                } else {\r\n                    console.log('error submit!!');\r\n                    return false;\r\n                }\r\n            });\r\n        },\r\n        resetForm(formName) {\r\n            this.$refs[formName].resetFields();\r\n        }\r\n    }\r\n})\r\n</script>\r\n\r\n<template>\r\n    <div>\r\n        <el-collapse v-model=\"vMode\">\r\n            <el-collapse-item title=\"下单流程\" name=\"1\">\r\n                <div><el-alert type=\"success\">填写订单表格=>电话转账或者到快递点付款=>收到下单的邮件=>将快件送到我们站点=>付款=>在首页查看订单状态</el-alert></div>\r\n                <div><el-alert type=\"success\">您可以选择将物品寄送到站点，然后您会收到付款邮件，并在邮件中付款</el-alert></div>\r\n                <div><el-alert type=\"success\">付款后订单的状态会改变</el-alert></div>\r\n            </el-collapse-item>\r\n            <el-collapse-item title=\"下单须知\" name=\"2\">\r\n                <div><el-alert type=\"error\">粉末类，易燃易爆类，液体类，烟膏，烟油，药品保健类拒收</el-alert></div>\r\n                <div><el-alert type=\"success\">一般时效12-15到达俄罗斯莫斯</el-alert></div>\r\n                <div><el-alert type=\"success\">订单号系统自动生成不需要填写</el-alert></div>\r\n                <div><el-alert type=\"warning\">下单后请及时上传照片</el-alert></div>\r\n            </el-collapse-item>\r\n        </el-collapse>\r\n\r\n        <el-card class=\"box-card\">\r\n            <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\">\r\n                <el-form-item label=\"订单号\" prop=\"name\" required>\r\n                    <el-input v-model=\"ruleForm.orderName\" disabled ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄货地址\" prop=\"sendRegion\" required>\r\n                    <el-select v-model=\"ruleForm.sendRegion\" placeholder=\"选择服务区\" style=\"width: 25%\">\r\n                        <el-option label=\"圣彼得堡\" value=\"stp\"></el-option>\r\n                        <el-option label=\"莫斯科\" value=\"moscow\"></el-option>\r\n                        <el-option label=\"广东\" value=\"guangDong\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄出时间\" required>\r\n                    <el-form-item prop=\"dateOut\">\r\n                        <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"ruleForm.dateOut\" style=\"width: 25%;\"></el-date-picker>\r\n                    </el-form-item>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"寄件数量\" prop=\"deliveryNum\">\r\n                    <template>\r\n                        <template>\r\n                            <el-input-number v-model=\"ruleForm.deliveryNum\" :step=\"1\" step-strictly></el-input-number>\r\n                        </template>\r\n                    </template>\r\n                </el-form-item>\r\n                <el-form-item label=\"其他\" prop=\"others\">\r\n                    <el-radio-group v-model=\"ruleForm.others\">\r\n                        <el-radio label=\"自提\"></el-radio>\r\n                        <el-radio label=\"保险\"></el-radio>\r\n                    </el-radio-group>\r\n                </el-form-item>\r\n                <el-form-item label=\"姓名\" prop=\"name\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.name\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人电话\" prop=\"phone\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.phone\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"寄件人邮箱\" prop=\"email\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.email\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"收货地址\" prop=\"receiveRegion\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入内容\"\r\n                        v-model=\"ruleForm.receiveRegion\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    >\r\n                    </el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"物品详情\" prop=\"details\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.details\" maxlength=\"50\"\r\n                              show-word-limit></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"备注\" prop=\"desc\">\r\n                    <el-input type=\"textarea\" v-model=\"ruleForm.desc\" maxlength=\"50\"\r\n                              show-word-limit></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"支付方式\" prop=\"desc\">\r\n                <el-row>\r\n                    <el-col :span=\"10\"><div class=\"grid-content bg-purple-dark\">\r\n                        <el-radio-group v-model=\"radio1\">\r\n                            <el-radio-button label=\"到店付款\"></el-radio-button>\r\n                            <el-radio-button label=\"在线转账\"></el-radio-button>\r\n                        </el-radio-group>\r\n                    </div></el-col>\r\n\r\n                    <el-col :span=\"24\"><div class=\"grid-content bg-purple-dark\">\r\n                        <el-tag  type=\"success\" size=\"medium \">通过手机转账12348734857</el-tag>\r\n                    </div></el-col>\r\n                </el-row>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">立即创建</el-button>\r\n                    <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AACA,SAAAA,eAAA;AAEA,eAAAA,eAAA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,QAAA;QACAC,SAAA;QACAC,UAAA;QACAC,OAAA;QACAC,WAAA;QACAC,MAAA;QACAP,IAAA;QACAQ,KAAA;QACAC,KAAA;QACAC,aAAA;QACAC,OAAA;QACAC,IAAA;MAEA;MACAC,KAAA;QACAb,IAAA,GACA;UAAAc,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAC,GAAA;UAAAH,OAAA;UAAAC,OAAA;QAAA,EACA;QACAG,MAAA,GACA;UAAAL,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAI,KAAA,GACA;UAAAC,IAAA;UAAAP,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAM,KAAA,GACA;UAAAD,IAAA;UAAAP,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAK,IAAA,GACA;UAAAA,IAAA;UAAAP,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAO,QAAA,GACA;UAAAT,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAJ,IAAA,GACA;UAAAE,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;MACAQ,KAAA;IACA;EACA;EACAC,QAAA,GAEA;EACAC,OAAA;IACAC,WAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACAC,KAAA;QACA;UACAC,OAAA,CAAAC,GAAA;UACA;QACA;MACA;IACA;IACAC,UAAAP,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAQ,WAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}