{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { setRoutes } from \"@/router\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      user: {},\n      rules: {\n        username: [{\n          required: true,\n          message: 'Пожалуйста, введите ваше имя пользователя',\n          trigger: 'blur'\n        }, {\n          min: 3,\n          max: 50,\n          message: 'длина от 3 до 50 символов',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: 'Пожалуйста, введите пароль',\n          trigger: 'blur'\n        }, {\n          min: 1,\n          max: 50,\n          message: 'Длина от 1 до 50 символов',\n          trigger: 'blur'\n        }]\n      },\n      tokenIswork: true\n    };\n  },\n  created() {\n    this.user = localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {};\n    if (this.tokenIswork) {\n      if (this.user.role) {\n        if (this.user.role === 'ROLE_USER') {\n          this.$router.push(\"/front/home\").catch(() => {});\n        } else {\n          this.$router.push(\"/manage/Dashbord\").catch(() => {});\n        }\n      }\n    }\n    this.user = {};\n  },\n  methods: {\n    changeTokenIswork(state) {\n      this.tokenIswork = state;\n    },\n    login() {\n      const loading = this.$loading({\n        lock: true,\n        text: 'Loading',\n        spinner: 'el-icon-loading',\n        background: 'rgba(0, 0, 0, 0.8)'\n      });\n      setTimeout(() => {\n        loading.close();\n      }, 10000);\n      this.$refs['userForm'].validate(valid => {\n        if (valid) {\n          // 表单校验合法\n          this.request.post(\"/user/login\", this.user).then(res => {\n            try {\n              if (res.code === '200') {\n                if (res.data != 'false') {\n                  localStorage.setItem(\"user\", JSON.stringify(res.data)); // 存储用户信息到浏览器\n                  localStorage.setItem(\"menus\", JSON.stringify(res.data.menus)); // 存储用户信息到浏览器\n                  this.changeTokenIswork(true);\n                  // 动态设置当前用户的路由\n                  setRoutes();\n                  this.$message.success(\"Вход в систему успешный\");\n                  if (res.data.role === 'ROLE_USER') {\n                    this.$router.push(\"/front/home\");\n                  } else {\n                    this.$router.push(\"/manage\").catch(() => {});\n                  }\n                  loading.close();\n                }\n              } else {\n                this.$message.error(\"Неправильное имя пользователя или пароль\");\n                loading.close();\n              }\n            } catch (e) {\n              this.$message.error(e);\n            }\n          });\n        } else {\n          loading.close();\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["setRoutes","name","data","user","rules","username","required","message","trigger","min","max","password","tokenIswork","created","localStorage","getItem","JSON","parse","role","$router","push","catch","methods","changeTokenIswork","state","login","loading","$loading","lock","text","spinner","background","setTimeout","close","$refs","validate","valid","request","post","then","res","code","setItem","stringify","menus","$message","success","error","e"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\r\n    <div class=\"wrapper\">\r\n        <nav class=\"navbar navbar-expand-lg navbar-transparent navbar-dark bg-dark py-4\">\r\n            <div class=\"container\">\r\n                <a class=\"navbar-brand\" href=\"../\"><strong>Aurexp</strong></a>\r\n                <button class=\"navbar-toggler\" type=\"button\" data-action=\"offcanvas-open\" data-target=\"#navbar_main\" aria-controls=\"navbar_main\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n                    <span class=\"navbar-toggler-icon\"></span>\r\n                </button>\r\n                <div class=\"navbar-collapse