.close();\r\n            }, 10000);\r\n            this.$refs['userForm'].validate((valid) => {\r\n                if (valid) {  // 表单校验合法\r\n                    this.request.post(\"/user/login\", this.user).then(res => {\r\n                        try {\r\n                            if(res.code === '200') {\r\n                                if(res.data != 'false'){\r\n                                    localStorage.setItem(\"user\", JSON.stringify(res.data))  // 存储用户信息到浏览器\r\n                                    localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))  // 存储用户信息到浏览器\r\n                                    this.changeTokenIswork(true)\r\n                                    // 动态设置当前用户的路由\r\n                                    setRoutes()\r\n                                    this.$message.success(\"Вход в систему успешный\")\r\n                                    if (res.data.role === 'ROLE_USER') {\r\n                                        this.$router.push(\"/front/home\")\r\n                                    } else {\r\n                                        this.$router.push(\"/manage\").catch(() =>{})\r\n                                    }\r\n                                    loading.close();\r\n                                }\r\n                            } else {\r\n                                this.$message.error(\"Неправильное имя пользователя или пароль\")\r\n                                loading.close();\r\n                            }\r\n                        } catch (e){\r\n                            this.$message.error(e)\r\n                        }\r\n                    })\r\n                } else {\r\n                    loading.close();\r\n                }\r\n            });\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"mappings":";AAmKA,SAAAA,SAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAA