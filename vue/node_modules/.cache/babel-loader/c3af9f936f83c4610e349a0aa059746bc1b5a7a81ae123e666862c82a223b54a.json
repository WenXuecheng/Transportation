           this.tableData[j][newProperty2].push(res.data[k].url);\n              }\n            }\n          }\n        });\n        ///保险自提\n        this.tableData[j].others = [this.tableData[j].others[0] + this.tableData[j].others[1], this.tableData[j].others[3] + this.tableData[j].others[4]];\n        //是否确认\n        if (this.tableData[j].isConfirmed === true) {\n          this.tableData[j].isConfirmed = '已确认';\n        } else {\n          this.tableData[j].isConfirmed = '待确认';\n        }\n        this.order = this.tableData[j];\n      });\n    },\n    updateExchangeRate() {\n      this.request.get(\"/others/1\").then(res => {\n        if (res.code == '200') {\n          this.exchangeRate = res.data;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","props","user","Object","data","exchangeRate","goodsOne","goodsTwo","order","tableData","n","created","searchOrdersByUsername","updateExchangeRate","methods","request","get","then","res","JSON","parse","stringify","length","newProperty1","newProperty2","j","i","id","orderName","k","adminUp","push","url","others","isConfirmed","code"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <template>\r\n            <div >\r\n                <el-row :gutter=\"20\">\r\n                    <el-col :span=\"6\">\r\n                        <div >\r\n                            <el-card class=\"box-card\">\r\n                                <el-statistic group-separator=\",\" :precision=\"2\" decimal-separator=\".\" :value=\"exchangeRate\" title={{this.n}}>\r\n                                    <template slot=\"prefix\">\r\n                                        <i class=\"el-icon-s-flag\" style=\"color: red\"></i>\r\n                                    </template>\r\n                                    <template slot=\"suffix\">\r\n                                        <i class=\"el-icon-s-flag\" style=\"color: blue\"></i>\r\n                                   